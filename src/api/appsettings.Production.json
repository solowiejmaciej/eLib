{
  "ConnectionStrings": {
    "DefaultConnection": "{{DB_CONNECTION_STRING}}"
  },
  "AuthSettings": {
    "SecretKey": "{{JWT_KEY}}"
  },
  "RabbitMQ": {
    "HostName": "{{RABBITMQ_HOSTNAME}}",
    "UserName": "{{RABBITMQ_USERNAME}}",
    "Password": "{{RABBITMQ_PASSWORD}}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
